{
  "name": "react-production-calendar",
  "version": "1.0.1",
  "description": "React production calendar on React.js",
  "main": "lib/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "buildextract": "cross-env NODE_ENV=production webpack --config config/webpack.prod.extract.js",
    "compile": "npm run compile-js && npm run compile-styles && npm run minifier",
    "compile-js": "babel src/calendar -d lib --copy-files && npm run minifier",
    "compile-styles": "node-sass --output-style compressed ./src/calendar/styles/ -o ./lib/styles/ ",
    "minifier": "terser ./lib/index.js -o ./lib/index.js -c -m && terser ./lib/components/calendarBody.js -o ./lib/components/calendarBody.js -c -m && terser ./lib/components/calendarDay.js -o ./lib/components/calendarDay.js -c -m && terser ./lib/components/calendarHead.js -o ./lib/components/calendarHead.js -c -m && terser ./lib/services/days.js -o ./lib/services/days.js -c -m && terser ./lib/services/month.js -o ./lib/services/month.js -c -m && terser ./lib/services/monthClass.js -o ./lib/services/monthClass.js -c -m && terser ./lib/services/time.js -o ./lib/services/time.js -c -m && terser ./lib/components/calendarBodyItem.js -o ./lib/components/calendarBodyItem.js -c -m",
    "test": "jest"
  },
  "author": "Maksim Zhuchkov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maksimkaJCHK/react-production-calendar"
  },
  "keywords": [
    "react",
    "reactjs",
    "calendar",
    "production calendar",
    "react calendar",
    "react production calendar"
  ],
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "chunks-webpack-plugin": "^7.1.0",
    "core-js": "^3.28.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.15",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.3",
    "jest-enzyme": "^7.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "sugarss": "^4.0.1",
    "terser-webpack-plugin": "^5.3.6",
    "twig-html-loader": "^0.1.9",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
